<% include partials/header %>

        <!-- Display New Job Box -->
        
            <div id="newJob" class="boxDialog">
                <section class="jobDetails">
                    <a href="#close" title="Close" class="close">X</a>
                    <div class="siteDetails">
                            <div class="divider">
                                <div class="form-group"><label for="jobRef">Job Ref</label> <input type="text" name="jobRef" /></div>
                                <div class="form-group"><label for="mprn">MPRN</label> <input type="text" name="mprn" /></div>
                                <div class="form-group"><label for="supplier">Supplier</label> <input type="text" name="supplier" /></div>
                            </div>
                            <div class="divider">
                                <div class="form-group"><label for="siteName">Site Name</label> <input type="text" name="siteName" /></div>
                                <div class="form-group"><label for="buildingNo">Building No</label> <input type="text" name="buildingNo" /></div>
                                <div class="form-group"><label for="streetAddress">Street (Line 1)</label> <input type="text" name="streetAddress" /></div>
                                <div class="form-group"><label for="streetAddress2">Street (Line 2)</label><input type="text" name="streetAddress2" /></div>
                            </div>
                            <div class="divider">
                                <div class="form-group"><label for="town">Town/City</label><input type="text" name="townCity" /></div>
                                <div class="form-group"><label for="postCode">Post Code</label><input type="text" name="postCode" /></div>
                                <div class="form-group"><label for="siteContact">Site Contact</label><input type="text" name="siteContact" /></div>
                                <div class="form-group"><label for="contactNo">Contact Number</label><input type="text" name="contactNo" /></div>
                            </div> 
                            <div class="divider">
                                <button class="btn btn-sm btn-warning buttonSize">Unlock Job</button>
                                <button class="btn btn-sm btn-danger buttonSize">Delete Job</button>
                            </div>
                    </div>
                    <hr>
                    <div class="assetDetails">
                            <div class="divider">
                                <div class="form-group"><label for="msn">Meter Serial No</label><input type="text" name="msn" /></div>
                                <div class="form-group"><label for="meterModel">Meter Model</label><input type="text" name="meterModel" /></div>
                                <div class="form-group"><label for="meterMake">Meter Make</label><input type="text" name="meterMake" /></div>
                            </div>
                            <div class="divider">
                                <div class="form-group"><label for="meterType">Meter Type</label><input type="text" name="meterType" /></div>
                                <div class="form-group"><label for="admSerial">ADM Serial</label><input type="text" name="admSerial" /></div>
                                <div class="form-group"><label for="admImei">ADM IMEI</label><input type="text" name="admImei" /></div>
                            </div>
                            <div class="divider">
                                <div class="form-group"><label for="meterRead">Meter Read</label><input type="text" name="meterRead" /></div>
                                <div class="form-group"><label for="user">User</label><input type="text" name="user" /></div>
                                
                            </div>
                    </div>
                    <hr>
                    <div class="visitDetails">
                            <div class="visitBoxes">
                                <div class="form-group">
                                    <label>Visit Category: </label>
                                    <input id="visitType1" type="radio" name="visitCat" value="signal" /><label for="visitType1">Signal</label>
                                    <input id="visitType2" type="radio" name="visitCat" value="readEsc" /><label for="visitType2">Read/Escalate</label>
                                    <input id="visitType3" type="radio" name="visitCat" value="readReq"/><label for="visitType3">Read Required</label>
                                    <input id="visitType4" type="radio" name="visitCat" value="install"/><label for="visitType4">Install</label>
                                    <input id="visitType5" type="radio" name="visitCat" value="removal"/><label for="visitType1">Removal</label>
                                </div>
                            </div>
                            <div class="visitNotes">
                                <label for="visitNotes">Notes: </label><textarea id="visitNotes" rows="8"></textarea>
                            </div>
                            <div class="visitSave">
                                <div class="divider"></div>
                                <div class="divider"></div>
                                <div class="divider"></div>
                                <div class="divider">
                                    <button class="btn btn-lg btn-success buttonSize">Save</button>
                                </div>
                            </div>
                    </div>
                                


                </section>  
            </div>
        
        <!-- Display Job Search Box -->
        
            <div id="searchJob" class="jumbotron">
                <h2>SMSDM Job Portal</h2>
                <section class="searchDisplay divider">
                    <div class="form-group"><label>Job Ref </label> <input type="text" name="jobRef" /></div>
                    <div class="form-group"><label>MPRN </label> <input type="text" name="jobRef" /></div>
                    <div class="form-group"><label>PostCode</label> <input type="text" name="postCode" /></div>
                </div>
            </div>
        
        
        
<% include partials/footer %>